{
  "name": "automart_api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node index.js",
    "test": "npm run drop && npm run configure-admin && nyc mocha ./test/**/**.js --require @babel/register --recursive --timeout 10000 --exit",
    "createTables": "babel-node ./services/createTables.js",
    "travis": "npm run createTables",
    "drop": "babel-node ./services/dropSchema.js && npm run schema && npm run createTables",
    "schema": "babel-node ./services/createSchema.js",
    "coverage": "nyc --reporter=lcov  npm test",
    "configure-admin": "babel-node ./maintest/main.js",
    "deploy": "npm run drop && node index.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint '**/*.js'",
    "lint:fix": "prettier-eslint '**/*.js' --write",
    "migrate:dev": "sequelize db:migrate:undo:all && sequelize db:migrate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "author": "willywax",
  "license": "ISC",
  "engines": {
    "node": "11.4.0",
    "npm": "6.9.0"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "pg": "^7.11.0",
    "sequelize": "^5.19.4",
    "sequelize-cli": "^5.5.1",
    "swagger-ui": "^3.22.3",
    "swagger-ui-express": "^4.0.6",
    "upgrade": "^1.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/register": "^7.6.2",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.3",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  }
}
